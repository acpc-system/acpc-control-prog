[Desktop Entry]
Type=Application
Name=GNOME Settings Daemon
Exec=gsettings set org.gnome.desktop.background picture-uri file:///etc/bg.jpg
Exec=gsettings set org.gnome.desktop.background picture-options spanned
OnlyShowIn=GNOME;Unity;
NoDisplay=true
X-GNOME-Autostart-Phase=Initialization
X-GNOME-Autostart-Notify=true
X-GNOME-AutoRestart=true
X-Ubuntu-Gettext-Domain=gnome-settings-daemon
